{
    "compilerOptions": {
      
      "target": "es5",
      "strict": true,
      "removeComments": true,
      "noImplicitAny": true,
      "preserveConstEnums": true,
      "sourceMap": true,
      "inlineSources": true,
      "declaration": true,
      "experimentalDecorators":true,
      "esModuleInterop":true,
      "resolveJsonModule":true,
      "types": ["node","jest"],
      "module": "es2015",
      "moduleResolution": "node"
    },
    
    
    "exclude": [
      "node_modules"
    ]
}